{
  "kind": "collectionType",
  "collectionName": "shops",
  "info": {
    "name": "Shop",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "reference": {
      "type": "string"
    },
    "Name": {
      "type": "string"
    },
    "pack_size": {
      "type": "integer",
      "default": 1,
      "min": 1
    },
    "pack_type": {
      "type": "enumeration",
      "enum": [
        "canule",
        "tip",
        "kit",
        "pin",
        "unite"
      ]
    },
    "price": {
      "type": "decimal",
      "min": 0,
      "default": 0
    },
    "discount": {
      "type": "decimal",
      "default": 0,
      "min": 0,
      "max": 100,
      "private": false
    },
    "pictures": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "relative": {
      "type": "enumeration",
      "enum": [
        "fractora",
        "morpheus",
        "bodytite",
        "votiva",
        "evolve"
      ]
    },
    "description": {
      "type": "text"
    }
  }
}
